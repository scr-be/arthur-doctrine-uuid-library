{
    "name"        : "scribe/arthur-doctrine-uuid",
    "license"     : "MIT",
    "type"        : "library",
    "homepage"    : "https://scr.be/arthur-doctrine-uuid-library",
    "description" : "A collection of objects/interfaces to simplify bundle setup, such as advanced bundle configuration, compiler passes, and deprecaton error handlers.",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://arthur-doctrine-uuid-library.oss.scr.be/issues",
        "source" : "https://arthur-doctrine-uuid-library.oss.scr.be/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"   : [
        "arthur-doctrine-uuid-library", "scribe-arthur-doctrine-uuid-library", "library", "scr.be", "scribe", "scribenet", "scribe-software", "scribe-systems"
    ],
    "authors"    : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@scr.be",
            "homepage": "https://scr.be/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require" : {
        "php"                  : ">=5.6.4,~5.6|~7.0",
        "scribe/wonka-library" : "~0.1",
        "doctrine/orm"         : "~2.5",
        "ramsey/uuid"          : "~3.0"
    },
    "require-dev" : {
        "phpunit/phpunit"             : "~5.0",
        "sensiolabs/security-checker" : "~3.0",
        "scrutinizer/ocular"          : "~1.1",
        "satooshi/php-coveralls"      : "~0.7@dev"
    },
    "suggest" : {
        "ramsey/uuid-console": "A Symfony console application for generating UUIDs with ramsey/uuid."
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\Doctrine\\" : "lib/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Scribe\\Doctrine\\Tests\\" : "tests/"
        }
    },
    "config": {
        "bin-dir"           : "bin",
        "minimum-stability" : "dev",
        "prefer-stable"     : true
    },
    "extra": {
        "branch-alias": {
            "dev-master" : "0.1-dev"
        }
    }
}
