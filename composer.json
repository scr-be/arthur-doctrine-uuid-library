{
    "name"        : "src-run/arthur-doctrine-uuid-library",
    "license"     : "MIT",
    "type"        : "library",
    "homepage"    : "https://get.src.run/arthur-doctrine-uuid-library",
    "description" : "A string and binary UUID-4 Doctrine ORM-type implementation as well as complimentary ORM-generator strategy implementations.",
    "support" : {
        "email"  : "rmf@src.run",
        "issues" : "https://get.src.run/arthur-doctrine-uuid-library/issues",
        "source" : "https://get.src.run/arthur-doctrine-uuid-library/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"   : [
        "arthur-doctrine-uuid-library", "scribe-arthur-doctrine-uuid-library", "library", "get.src.run", "scribe", "scribenet", "scribe-software", "scribe-systems"
    ],
    "authors"    : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@src.run",
            "homepage": "https://get.src.run/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require" : {
        "php"                                   : "~5.6|~7.0",
        "src-run/arthur-doctrine-utils-library" : "~0.5",
        "ramsey/uuid"                           : "~3.0"
    },
    "require-dev" : {
        "doctrine/orm"           : "~2.5",
        "doctrine/dbal"          : "~2.5",
        "phpunit/phpunit"        : "~5.0",
        "codacy/coverage"        : "~1.0",
        "satooshi/php-coveralls" : "~1.0"
    },
    "suggest" : {
        "doctrine/orm"        : "~2.5",
        "doctrine/dbal"       : "~2.5",
        "ramsey/uuid-console" : "A Symfony console application for generating UUIDs with ramsey/uuid."
    },
    "autoload" : {
        "psr-4" : {
            "SR\\Doctrine\\ORM\\Id\\"     : "lib/ORM/Id/",
            "SR\\Doctrine\\DBAL\\Types\\" : "lib/DBAL/Types/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "SR\\Doctrine\\Tests\\ORM\\Id\\"     : "tests/ORM/Id/",
            "SR\\Doctrine\\Tests\\DBAL\\Types\\" : "tests/DBAL/Types/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "config": {
        "bin-dir" : "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master" : "0.5-dev"
        }
    }
}
